{
    "ignores": ["toPyQt"],

    "replacements": {
        "Organizer::FileInfo": "FileInfo",
        "IOrganizer::FileInfo": "FileInfo",
        "IPluginInstaller::EInstallResult": "InstallResult"
    },

    "overwrites": {

        "IFileTree": {
            "__iter__": {
                "rtype": "Iterator[FileTreeEntry]",
                "args": [],
                "static": false,
                "overloads": false
            }
        },

        "IDownloadManager": {
            "[bases]": ["PyQt5.QtCore.QObject"],
            "[signals]": [
                "downloadComplete[int]",
                "downloadPaused[int]",
                "downloadFailed[int]",
                "downloadRemoved[int]"
            ]
        },

        "IModRepositoryBridge": {
            "[bases]": ["PyQt5.QtCore.QObject"],
            "[signals]": [
                "descriptionAvailable[str, int, Any, Any]",
                "filesAvailable[str, int, Any, List[ModRepositoryFileInfo]]",
                "fileInfoAvailable[str, int, int, Any, Any]",
                "downloadURLsAvailable[str, int, int Any, Any]",
                "endorsementsAvailable[Any, Any]",
                "endorsementToggled[str, int, Any, Any]",
                "trackedModsAvailable[Any, Any]",
                "trackingToggled[str, int, Any, bool]",
                "requestFailed[str, int, int, Any, NetworkError, str]"
            ]
        },

        "FileInfo": {
            "[properties]": {
                "filePath": "str",
                "archive": "str",
                "origins": "List[str]"
            }
        },

        "Mapping": {
            "[properties]": {
                "source": "str",
                "destination": "str",
                "isDirectory": "bool",
                "createTarget": "bool"
            }
        },

        "ModRepositoryFileInfo": {
            "[properties]": {
                "name": "str",
                "uri": "str",
                "description": "str",
                "version": "VersionInfo",
                "newestVersion": "VersionInfo",
                "categoryID": "int",
                "modName": "str",
                "gameName": "str",
                "modID": "int",
                "fileID": "int",
                "fileSize": "int",
                "fileName": "str",
                "fileCategory": "int",
                "fileTime": "PyQt5.QtCore.QDateTime",
                "repository": "str",
                "userData": "PyQt5.QtCore.QVariant"
            }
        }
    }
}
